	<munit:config name="leads-test-suite.xml" />
	<munit:test name="post-leads-flowTest" doc:id="gdohnt78-r9sa-p998-azhk-lxasme5wse" description="Test">
		<munit:behavior >
			<munit-tools:mock-when doc:name="Mock when" doc:id="7j7it4nh-p816-ytx6-ccdv-p6ek4rgzl2g" processor="salesforce:create">
				<munit-tools:with-attributes >
					<munit-tools:with-attribute whereValue="doc-da025bf9" attributeName="doc:id" />
				</munit-tools:with-attributes>
				<munit-tools:then-return >
					<munit-tools:payload value='#[{
    "id": null,
    "items": [
        {
            "exception": null,
            "message": null,
            "payload": {
                "success": true,
                "id": "001xx000003AAA",
                "errors": []
            },
            "id": "001xx000003AAA",
            "statusCode": null,
            "successful": true
        }
    ],
    "successful": true
}]' />
				</munit-tools:then-return>
			</munit-tools:mock-when>
		</munit:behavior>
		<munit:execution >
			<flow-ref doc:name="post-leads-flow" doc:id="4k6lcetu-zwc2-ug1a-2xxm-e0a0nlbjj2" name="post-leads-flow"/>
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert equals" doc:id="m5xhxvtl-0gss-3tye-o0zh-140amgakuynd" actual="#[payload]" expected='#[{
  success: payload.success,
  id: payload.id,
  message: payload.message,
  errors: payload.errors
}]'/>
		</munit:validation>
	</munit:test>
